{
  "extends": [
    "config:js-app",
    ":automergeAll",
    ":disableRateLimiting",
    ":rebaseStalePrs",
    ":masterIssue"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchPackageNames": ["knex"],
      "allowedVersions": "< 0.22.0"
    },
    {
      "packageNames": [
        "node"
      ],
      "enabled": false
    }
  ],
  "prCreation": "not-pending",
  "stabilityDays": 3
}
